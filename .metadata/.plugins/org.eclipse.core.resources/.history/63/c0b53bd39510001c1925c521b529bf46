public class Demo {
	public static void main(String[] args) {
		System.out.println("hoi");
		int getal = 3;
		System.out.println(getal);
		
		Huis huis1 = new Huis();
		huis1.hoogte = 12;
		huis1.verkopen();
		System.out.println(huis1.verkopen());
	}
}

class Huis {
	int hoogte;
	String verkopen() {
		System.out.println("Het huis wordt verkocht!");
		return "Whatever je string is";
	}
}